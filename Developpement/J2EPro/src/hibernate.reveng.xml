<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="REAL" hibernate-type="java.lang.Float"
			precision="3" not-null="false">
		</sql-type>
		<sql-type jdbc-type="DATE" hibernate-type="java.util.Date"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="BIT" hibernate-type="boolean"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP"
			hibernate-type="java.sql.Timestamp" not-null="false">
		</sql-type>
	</type-mapping>
	<table-filter match-name="Client" match-catalog="j2ee"></table-filter>
	<table-filter match-name="Commande" match-catalog="j2ee"></table-filter>
	<table-filter match-name="Menu" match-catalog="j2ee"></table-filter>
	<table-filter match-name="Panier" match-catalog="j2ee"></table-filter>
	<table-filter match-name="Plat" match-catalog="j2ee"></table-filter>
	<table-filter match-name="Produit" match-catalog="j2ee"></table-filter>
	<table-filter match-name="Traiteur" match-catalog="j2ee"></table-filter>
	<table catalog="j2ee" name="Panier">
		<column name="id_panier"></column>
		<column name="id_cli"></column>
	</table>
</hibernate-reverse-engineering>